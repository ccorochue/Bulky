@model List<Category>

<h1>Category list</h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>
                Category
            </th>
            <th>
                Display order
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model.OrderBy(u => u.display_order))
        {
            <tr>
                <td>@obj.name</td>
                <td>@obj.display_order</td>
            </tr>
        }
    </tbody>
</table>